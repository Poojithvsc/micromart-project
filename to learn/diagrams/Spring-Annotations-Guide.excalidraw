    {
      "id": "example-code",
      "type": "text",
      "x": 50,
      "y": 770,
      "width": 1140,
      "height": 85,
      "text": "@RestController                                    // Marks as REST controller, returns JSON\n@RequestMapping(\"/api/users\")                      // Base path for all endpoints\n@RequiredArgsConstructor                           // Lombok: constructor for final fields\npublic class UserController {\n    private final UserService userService;         // Injected via constructor\n    \n    @PostMapping                                   // HTTP POST to /api/users\n    public ResponseEntity<ApiResponse<UserResponse>> createUser(@Valid @RequestBody CreateUserRequest request) {\n        return ResponseEntity.ok(ApiResponse.success(userService.createUser(request)));    // Returns 200 OK with wrapped response\n    }\n}",
